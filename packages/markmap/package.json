{
  "name": "@vitepress-plugin/markmap",
  "version": "1.1.3",
  "description": "VitePress 插件 用 Markdown 绘制交互式脑图",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js"
    },
    "./markmap": {
      "types": "./dist/index.d.ts",
      "import": "./dist/markmap.es.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm clean && vite build && tsc --emitDeclarationOnly",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "markmap",
    "mindmap",
    "custom-container"
  ],
  "author": {
    "name": "PaddyWang",
    "email": "1303841222@qq.com",
    "url": "https://github.com/PaddyWang"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaddyWang/vitepress-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/PaddyWang/vitepress-plugin/issues",
    "email": "1303841222@qq.com"
  },
  "homepage": "http://paddywang.github.io/vitepress-plugin/markmap",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "markmap-lib": "^0.18.12",
    "markmap-toolbar": "^0.18.12",
    "markmap-view": "^0.18.12"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "rimraf": "^6.1.2",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  }
}
